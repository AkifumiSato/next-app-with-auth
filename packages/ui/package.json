{
  "name": "ui",
  "version": "0.0.0",
  "private": true,
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./src/index.tsx"
    },
    "./index.css": "./dist/index.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.tsx",
    "dev": "tsup src/index.tsx --watch",
    "lint": "eslint .",
    "generate:component": "turbo gen react-component"
  },
  "devDependencies": {
    "@turbo/gen": "1.10.16",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "react": "18.2.0",
    "autoprefixer": "10.4.16",
    "eslint-config-custom": "workspace:*",
    "postcss": "8.4.31",
    "tailwindcss": "3.3.5",
    "tsconfig": "workspace:*",
    "tsup": "7.2.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "react": ">=18.2.0"
  }
}
